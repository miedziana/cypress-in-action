{
  "name": "client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "prod": "node server.js",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "e2e": "ng e2e",
    "coverage-report": "nyc report --reporter=cobertura --report-dir=reports",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "start:ci": "ng run client:serve-coverage",
    "e2e:ci": "ng run client:e2e-ci",
    "e2e:coverage": "npx nyc report --reporter=lcov --reporter=text-summary",
    "copy:reports": "(mkdir reports || true) && cp coverage-jest/coverage-final.json reports/from-jest.json && cp .nyc_output/out.json reports/from-cypress.json",
    "combine:reports": "npx nyc merge reports && mv coverage.json .nyc_output/out.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.0",
    "@angular/cdk": "^14.2.3",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/material": "^14.2.3",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/router": "^14.2.0",
    "@auth0/auth0-angular": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "http-proxy-middleware": "^2.0.6",
    "jsonwebtoken": "^8.5.1",
    "path": "^0.12.7",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^14.0.1",
    "@angular-devkit/build-angular": "^14.2.4",
    "@angular/cli": "~14.2.4",
    "@angular/compiler-cli": "^14.2.0",
    "@cypress/code-coverage": "^3.10.0",
    "@cypress/schematic": "^2.1.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@jsdevtools/coverage-istanbul-loader": "^3.0.5",
    "@types/jest": "^29.2.1",
    "cypress": "latest",
    "istanbul-lib-coverage": "^3.2.0",
    "jest": "^29.2.2",
    "jest-junit": "^14.0.1",
    "jest-preset-angular": "^12.2.2",
    "nyc": "^15.1.0",
    "typescript": "~4.7.2",
    "webpack": "^5.74.0"
  }
}
